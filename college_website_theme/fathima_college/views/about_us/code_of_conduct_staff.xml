            <field name="type">qweb</field>
            <field name="key">fathima_college.code_of_conduct_staff</field>
            <field name="arch" type="xml">
                <t name="Code Of Conduct" t-name="fathima_college.code_of_conduct_staff_menu">
                    <t t-call="website.layout">
                        <div id="wrap" class="oe_structure">
                            <section class="pt32 pb32 o_colored_level" style="background-image: none;">
                                <div class="container">
                                    <div class="about_head s_header">Code Of Conduct Staff
                                        <div class="s_line">
                                            <div class="s_dot"/>
                                        </div>
                                    </div>
                                </div>
                            </section>
                        </div>
                    </t>
                </t>
            </field>
        </record>
        <record id="code_of_conduct_staff_page" model="website.page">
            <field name="url">/code_of_conduct_staff</field>
            <field name="is_published">True</field>
            <field name="view_id" ref="code_of_conduct_staff"/>
            <field name="track">True</field>
