<odoo>
    <data>
        <record id="former-principals" model="ir.ui.view">
            <field name="name">former-principals</field>
            <field name="type">qweb</field>
            <field name="key">fathima_college.former-principals</field>
            <field name="arch" type="xml">
                <t name="Staff Council" t-name="fathima_college.former-principals_menu">
                    <t t-call="website.layout">
                        <div id="wrap" class="oe_structure">
                            <section class="pt32 pb32 o_colored_level" style="background-image: none;">
                                <div class="container">
                                    <div class="about_head s_header">Former Principals
                                        <div class="s_line">
                                            <div class="s_dot"/>
                                        </div>
                                    </div>
                                    <section class="pt32 pb32 managers" id="members">
                                        <div class="row">
                                            <div class="card others" id="others">
                                                <div class="img">
                                                    <img src="/fathima_college/static/img/profile.jpg" alt=""
                                                         class="img img-fluid o_we_custom_image"
                                                         data-original-id="/fathima_college/static/img/profile.jpg"
                                                         data-mimetype="image/png" data-resize-width="286"
                                                         loading="lazy" id="others_images"/>
                                                </div>
                                                <div id="others_content">
                                                    <h2 class="naming">Name</h2>
                                                    <span>Designation</span>
                                                </div>
                                            </div>
                                            <div class="card others" id="others">
                                                <div class="img">
                                                    <img src="/fathima_college/static/img/profile.jpg" alt=""
                                                         class="img img-fluid o_we_custom_image"
                                                         data-original-id="/fathima_college/static/img/profile.jpg"
                                                         data-mimetype="image/png" data-resize-width="286"
                                                         loading="lazy" id="others_images"/>
                                                </div>
                                                <div id="others_content">
                                                    <h2 class="naming">Name</h2>
                                                    <span>Designation</span>
                                                </div>
                                            </div>
                                            <div class="card others" id="others">
                                                <div class="img">
                                                    <img src="/fathima_college/static/img/profile.jpg" alt=""
                                                         class="img img-fluid o_we_custom_image"
                                                         data-original-id="/fathima_college/static/img/profile.jpg"
                                                         data-mimetype="image/png" data-resize-width="286"
                                                         loading="lazy" id="others_images"/>
                                                </div>
                                                <div id="others_content">
                                                    <h2 class="naming">Name</h2>
                                                    <span>Designation</span>
                                                </div>
                                            </div>
                                        </div>
                                    </section>
                                </div>
                            </section>
                        </div>
                    </t>
                </t>
            </field>
        </record>
        <record id="former-principals_page" model="website.page">
            <field name="url">/former-principals</field>
            <field name="is_published">True</field>
            <field name="view_id" ref="former-principals"/>
            <field name="track">True</field>
        </record>
    </data>
</odoo>