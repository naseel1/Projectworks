<odoo>
    <data>
        <record id="admission_procedure" model="ir.ui.view">
            <field name="name">Admission Procedure</field>
            <field name="type">qweb</field>
            <field name="key">fathima_college.admission_procedure</field>
            <field name="arch" type="xml">
                <t name="About Us" t-name="fathima_college.admission_procedure_menu">
                    <t t-call="website.layout">
                        <div id="wrap" class="oe_structure">
                            <section class="pt32 pb32 o_colored_level" style="background-image: none;">
                                <div class="container">
                                    <div class="about_head s_header">Admission Procedure
                                        <div class="s_line">
                                            <div class="s_dot"/>
                                        </div>
                                    </div>
                                    <div class=" s_header" id="sub-header">Introduction
                                        <div class="s_line">
                                            <div class="s_dot"/>
                                        </div>
                                    </div>

                                    <div class="about_paragraph">
                                        Embarking on a journey of higher education at Fathima Arts &amp; Science College
                                        is
                                        a straightforward and well-structured process designed to ensure a smooth
                                        transition for prospective students. The admission procedure at Fathima Arts
                                        &amp;
                                        Science College is meticulously crafted to identify and welcome talented
                                        individuals who are eager to pursue their academic goals in an environment that
                                        promotes excellence, innovation, and holistic development.
                                    </div>
                                    <section class="pt16" id="eligibility_criteria">
                                        <div class=" s_header" id="sub-header">Eligibility Criteria
                                            <div class="s_line">
                                                <div class="s_dot"/>
                                            </div>
                                        </div>
                                        <div class="about_paragraph">
                                            Graduation in any subject in the Faculties of Arts, Social Sciences,
                                            Science, Oriental Studies and Commerce securing not less than 45% marks in
                                            Part III â€“ Optional Subjects. BBA Degree holders are also eligible.
                                        </div>
                                    </section>

                                </div>
                            </section>
                        </div>
                    </t>
                </t>
            </field>
        </record>
        <record id="admission_procedure_page" model="website.page">
            <field name="url">/admission_procedure</field>
            <field name="is_published">True</field>
            <field name="view_id" ref="admission_procedure"/>
            <field name="track">True</field>
        </record>
    </data>
</odoo>