<odoo>
    <data>
        <record id="commerce_activities" model="ir.ui.view">
            <field name="name">Commerce Activities</field>
            <field name="type">qweb</field>
            <field name="key">fathima_college.commerce_activities</field>
            <field name="arch" type="xml">
                <t name="BBA" t-name="fathima_college.commerce_activities_menu">
                    <t t-call="website.layout">
                        <div id="wrap" class="oe_structure">
                            <section class="pt32 pb32 o_colored_level" style="background-image: none;">
                                <div class="container">
                                    <div class="about_head s_header">Department Of Commerce &amp; Management
                                        <div class="s_line">
                                            <div class="s_dot"/>
                                        </div>
                                    </div>
                                    <section class="administration pt32  pb o_colored_level tabs_back tabs_back"
                                             data-vcss="001"
                                             data-snippet="s_tabs" data-name="Tabs" style="background-image: none;">
                                        <div class=" row " id="department_row">
                                            <div class="col-12 col-md-3 o_colored_level " id="side_section"
                                                 data-original-title="" title="" aria-describedby="tooltip336631">

                                                <div class="side-bar " id="sidebar"
                                                     style="top: 5px;margin-bottom: 0;">
                                                    <ul class=" nav nav-pills  flex-column"
                                                        style="padding-inline-start: 0;">
                                                        <li id="sidebar-li" class="nav-item">
                                                            <a href="/commerce" class="nav-link ">
                                                                Department Profile
                                                            </a>
                                                        </li>
                                                        <li id="sidebar-li" class="nav-item">
                                                            <a href="/commerce/faculty" class="nav-link ">
                                                                Faculty
                                                            </a>
                                                        </li>
                                                        <li id="sidebar-li" class="nav-item">
                                                            <a href="/commerce/programmes" class="nav-link ">
                                                                Programmes
                                                            </a>
                                                        </li>
                                                        <li id="sidebar-li" class="nav-item">
                                                            <a href="/commerce/activities" class="nav-link active">
                                                                Activities
                                                            </a>
                                                        </li>
                                                        <li id="sidebar-li" class="nav-item">
                                                            <a href="/commerce/report" class="nav-link ">
                                                                Reports
                                                            </a>
                                                        </li>
                                                    </ul>
                                                </div>
                                            </div>
                                            <div class="col-12 col-md-9 order-1 order-md-2 second_section pb48 o_colored_level">
                                                <div class="oe_structure oe_empty"
                                                     data-editor-sage="DRAG BUILDING BLOCKS HERE"
                                                     data-editor-message-default="true"
                                                     data-editor-message="DRAG BUILDING BLOCKS HERE">
                                                    <section id="administration" style="background-image: none;"
                                                             class="o_colored_level">
                                                        <div class=" s_header pb24" id="header_small">Activities
                                                            <div class="s_line">
                                                                <div class="s_dot"/>
                                                            </div>
                                                        </div>
                                                        <section data-snippet="s_blog_posts"
                                                                 class="s_blog_posts s_dynamic_snippet_blog_posts s_dynamic o_colored_level s_blog_post_fathima_department o_dynamic_empty"
                                                                 data-name="Blog Posts"
                                                                 style="background-image: none;min-height:200px;"
                                                                 data-filter-by-blog-id="10" data-number-of-records="6"
                                                                 data-number-of-elements="3" data-filter-id="1"
                                                                 data-template-key="fathima_college_news_event.dynamic_filter_template_blog_post_fathima"
                                                                 data-force-minimum-max-limit-to16="1">
                                                            <div class="o_not_editable container">
                                                                <div class="css_non_editable_mode_hidden">
                                                                    <div class="missing_option_warning alert alert-info rounded-0 fade show d-none d-print-none o_default_snippet_text">
                                                                        Your Dynamic Snippet will be displayed here...
                                                                        This message is displayed because you
                                                                        did not provided both a filter and a template to
                                                                        use.
                                                                        <br/>
                                                                    </div>
                                                                </div>
                                                                <div class="dynamic_snippet_template"/>
                                                            </div>
                                                        </section>
                                                        <div class="top_contact" id="more">
                                                            <a href="/blog/commerce-activities-4" id="readmore">View
                                                                More
                                                            </a>
                                                        </div>
                                                    </section>
                                                </div>
                                            </div>
                                        </div>
                                    </section>
                                </div>
                            </section>
                        </div>
                    </t>
                </t>
            </field>
        </record>
        <record id="commerce_activities_page" model="website.page">
            <field name="url">/commerce/activities</field>
            <field name="is_published">True</field>
            <field name="view_id" ref="commerce_activities"/>
            <field name="track">True</field>
        </record>
    </data>
</odoo>