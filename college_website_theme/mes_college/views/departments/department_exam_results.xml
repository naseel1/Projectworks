<odoo>
    <data>
        <record id="department_exam_results" model="ir.ui.view">
            <field name="name">Department Exam Results</field>
            <field name="type">qweb</field>
            <field name="key">mes_college.department_exam_results</field>
            <field name="arch" type="xml">
                <t name="v" t-name="mes_college.department_exam_results_menu">
                    <t t-call="website.layout">
                        <div id="wrap" class="oe_structure">
                            <section class="page o_colored_level" style="padding: 0px; background-image: none;">
                                <div class="bg_color_club" data-original-title="" title=""
                                     aria-describedby="tooltip229435">
                                </div>
                                <section class="s_tabs pb o_colored_level" data-vcss="001" data-snippet="s_tabs"
                                         data-name="Tabs" style="background-image: none;">
                                    <div class="s_tabs_main row " style="margin: 0 32px;">
                                        <t t-if="data">
                                            <div class="col-12 col-md-3 o_colored_level s_tabs_nav" id="side_section"
                                                 data-original-title=""
                                                 title="" aria-describedby="tooltip336631">
                                                <t t-if="data">
                                                    <div class="clubs">
                                                        <t t-esc="data['name']"/>
                                                    </div>
                                                    <div class="side-bar" id="sidebar">
                                                        <ul class="tabs-list1 nav nav-pills  flex-column"
                                                            style="padding-inline-start: 0;">
                                                            <li id="sidebar-li" class="nav-item">
                                                                <t t-if="data">
                                                                    <a t-att-href="'/department/' + str(data['dep_id'])"
                                                                       data-original-title="" title=""
                                                                       class="nav-link ">
                                                                        Home
                                                                    </a>
                                                                </t>
                                                            </li>
                                                            <li id="sidebar-li" class="nav-item">
                                                                <t t-if="data">

                                                                    <a t-att-href="'/department/' + str(data['dep_id']) + '/faculty'"
                                                                       data-original-title="" title="" class="nav-link">
                                                                        Faculties
                                                                    </a>
                                                                </t>
                                                            </li>
                                                            <li id="sidebar-li" class="nav-item">
                                                                <t t-if="data">
                                                                    <a t-att-href="'/department/' + str(data['dep_id']) + '/programme?dep_id=' + str(data['dep_id'])"
                                                                       data-original-title="" title=""
                                                                       class="nav-link ">
                                                                        Programmes
                                                                    </a>
                                                                </t>
                                                            </li>
                                                            <li id="sidebar-li" class="nav-item">
                                                                <t t-if="data">
                                                                    <a href="/chemistry_reports"
                                                                       data-original-title="" title="" class="nav-link">
                                                                        Reports
                                                                    </a>
                                                                </t>
                                                            </li>
                                                            <li id="sidebar-li" class="nav-item">
                                                                <t t-if="data">
                                                                    <a t-att-href="'/department/' + str(data['dep_id']) + '/activities'"
                                                                       data-original-title="" title=""
                                                                       class="nav-link ">
                                                                        Activities
                                                                    </a>
                                                                </t>
                                                            </li>
                                                            <li id="sidebar-li" class="nav-item">
                                                                <t t-if="data">
                                                                    <a t-att-href="'/department/' + str(data['dep_id']) + '/facilities?dep_id=' + str(data['dep_id'])"
                                                                       data-original-title="" title="" class="nav-link">
                                                                        Facilities
                                                                    </a>
                                                                </t>
                                                            </li>
                                                            <li id="sidebar-li" class="nav-item">
                                                                <t t-if="data">
                                                                    <a href="'/department/' + str(data['dep_id']) + '/exam_results'"
                                                                       data-original-title="" title=""
                                                                       class="nav-link active">
                                                                        Exam Results
                                                                    </a>
                                                                </t>
                                                            </li>
                                                            <li id="sidebar-li" class="nav-item">
                                                                <t t-if="data">
                                                                    <a t-att-href="'/department/' + str(data['dep_id']) + '/dep_gallery'"
                                                                       data-original-title="" title=""
                                                                       class="nav-link ">
                                                                        Gallery
                                                                    </a>
                                                                </t>
                                                            </li>
                                                        </ul>
                                                    </div>
                                                </t>
                                            </div>
                                        </t>
                                        <div class="col-12 col-md-9 order-1 order-md-2 pt48 pb48 o_colored_level">
                                            <div class="oe_structure oe_empty"
                                                 data-editor-sage="DRAG BUILDING BLOCKS HERE">
                                                <section id="departments_page">
                                                    <h2 class="page_head" id="department_subhead">Exam Results
                                                    </h2>
                                                    <div class="col-md-12 pt16">
                                                        <div id="myCollapse1700712777280" class="accordion"
                                                             role="tablist">
                                                            <t t-foreach="exam_result" t-as="results">
                                                                <div class="card" id="project_card" data-name="Item">
                                                                    <a href="#" role="tab" data-bs-toggle="collapse"
                                                                       aria-expanded="true"
                                                                       class="card-header s_faq_collapse_right_icon collapsed"
                                                                       id="project_head"
                                                                       t-att-data-bs-target="'#card' + str(results['id'])">
                                                                        <t t-if="results['batch_name']">
                                                                            <t t-esc="results['batch_name']"/>
                                                                        </t>
                                                                    </a>
                                                                    <div class="collapse "
                                                                         data-parent="#myCollapse1700712777280"
                                                                         role="tabpanel"
                                                                         t-att-id="'card' + str(results['id'])">
                                                                        <div class="card-body p-5">
                                                                            <table class="milestone  pt32"
                                                                                   id="researches">
                                                                                <tbody>
                                                                                    <t t-set="counter"
                                                                                       t-value="1"/>
                                                                                    <tr>
                                                                                        <th style="border-top-left-radius: 10px;">
                                                                                            No.
                                                                                        </th>
                                                                                        <th>
                                                                                            Semester
                                                                                        </th>
                                                                                        <th style="border-top-right-radius: 10px;">
                                                                                            View
                                                                                        </th>
                                                                                    </tr>
                                                                                    <t t-foreach="results.get('batch_terms', [])"
                                                                                       t-as="result_data">
                                                                                        <tr>
                                                                                            <td>
                                                                                                <t t-esc="counter"/>
                                                                                                <t t-set="counter"
                                                                                                   t-value="counter + 1"/>
                                                                                            </td>
                                                                                            <td style="border-top-right-radius:10px;">
                                                                                                Semester
                                                                                                <t t-esc="result_data['term']"/>
                                                                                            </td>
                                                                                            <td style="border-top-right-radius:10px;">
                                                                                                <a t-att-href="'/batches/' + str(result_data['id']) + '/semester/' + str(result_data['term']) + '/internals'"
                                                                                                   class="view_result">
                                                                                                    View
                                                                                                    <span class="fa fa-long-arrow-right "
                                                                                                          style="color:var(--primary);"
                                                                                                          data-bs-original-title=""
                                                                                                          title=""
                                                                                                          aria-describedby="tooltip544395"></span>
                                                                                                </a>
                                                                                            </td>
                                                                                        </tr>
                                                                                    </t>

                                                                                </tbody>

                                                                            </table>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </t>
                                                        </div>
                                                    </div>

                                                </section>


                                            </div>

                                        </div>

                                    </div>
                                </section>
                            </section>
                        </div>
                    </t>
                </t>
            </field>
        </record>
        <record id="department_exam_results_page" model="website.page">
            <field name="url">/exam</field>
            <field name="is_published">True</field>
            <field name="view_id" ref="department_exam_results"/>
            <field name="track">True</field>
        </record>
    </data>
</odoo>

