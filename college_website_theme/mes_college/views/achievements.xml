<odoo>
    <data>
        <record id="achievements" model="ir.ui.view">
            <field name="name">achievements</field>
            <field name="type">qweb</field>
            <field name="key">mes_college.achievements</field>
            <field name="arch" type="xml">
                <t name="Clubs" t-name="mes_college.achievements_menu">
                    <t t-call="website.layout">
                        <div id="wrap" class="oe_structure">
                            <section class="page o_colored_level" style="padding: 0px; background-image: none;">
                                <div class="container" style="max-width: 95%;">
                                    <h2 class="main-head" id="title" style="text-align:left;padding-left: 30px;">
                                        Achievements
                                    </h2>

                                </div>
                            </section>
                        </div>
                    </t>
                </t>
            </field>
        </record>
        <record id="achievements_page" model="website.page">
            <field name="url">/achievements</field>
            <field name="is_published">True</field>
            <field name="view_id" ref="achievements"/>
            <field name="track">True</field>
        </record>
    </data>
</odoo>