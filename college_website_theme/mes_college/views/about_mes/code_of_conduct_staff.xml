<odoo>
    <data>
        <record id="code_of_conduct_staff" model="ir.ui.view">
            <field name="name">Code Of Conduct Staff</field>
            <field name="type">qweb</field>
            <field name="key">mes_college.code_of_conduct_staff</field>
            <field name="arch" type="xml">
                <t name="graduate-attribute" t-name="mes_college.code_of_conduct_staff_menu">
                    <t t-call="website.layout">
                        <div id="wrap" class="oe_structure">
                            <section class="bg_color">
                                <div class="container" style="max-width: 95%;">
                                    <h2 class="main-head" id="title">Code Of Conduct Staff</h2>
                                </div>
                                <embed name="Code of Conducts Staff"
                                       src="/mes_college/static/img/code-of-conduct-staff.pdf"
                                       frameborder="0" width="100%" height="600"/>
                            </section>
                        </div>
                    </t>
                </t>
            </field>
        </record>
        <record id="code_of_conduct_staff_page" model="website.page">
            <field name="url">/code_of_conduct_staff</field>
            <field name="is_published">True</field>
            <field name="view_id" ref="code_of_conduct_staff"/>
            <field name="track">True</field>
        </record>
    </data>
</odoo>