<odoo>
    <data>
        <record id="alumni_registration" model="ir.ui.view">
            <field name="name">alumni_registration</field>
            <field name="type">qweb</field>
            <field name="key">mes_college.alumni_registration</field>
            <field name="arch" type="xml">
                <t name="english" t-name="mes_college.alumni_registration_menu">
                    <t t-call="website.layout">
                        <div id="wrap" class="oe_structure">
                            <section class="bg_color">
                                <div class="container" id="container_width">
                                    <div class="row">
                                        <div class="col-md-9 pb16">
                                            <h2 class="main-head" style="text-align:left; padding-top:20px;">
                                                Alumni Registration
                                            </h2>
                                            <div class="content_text" style="text-align:justify;padding: 0;">
                                                <section class="s_website_form pb16 o_colored_level pt56"
                                                         data-vcss="001" data-snippet="s_website_form" data-name="Form"
                                                         style="background-image: none;">
                                                    <div class="container-fluid" style="&#10;">
                                                        <form action="/website/form/" method="post"
                                                              enctype="multipart/form-data" class="o_mark_required"
                                                              data-mark="" data-pre-fill="true"
                                                              data-success-mode="redirect"
                                                              data-success-page="/contactus-thank-you"
                                                              data-model_name="mail.mail" style="&#10;">
                                                            <div class="s_website_form_rows row s_col_no_bgcolor"
                                                                 style="">


                                                                <div data-visibility-condition=""
                                                                     data-visibility-between=""
                                                                     class="s_website_form_field mb-3 col-12 s_website_form_custom s_website_form_required"
                                                                     data-type="char" data-name="Field"
                                                                     style="&#10;    display: flex;&#10;    width: auto;&#10;">
                                                                    <div class="row s_col_no_resize s_col_no_bgcolor">
                                                                        <label class="col-form-label col-sm-auto s_website_form_label"
                                                                               for="ob85dx1xebj5" style="width: 230px;">


                                                                            <span class="s_website_form_label_content">
                                                                                Name
                                                                            </span>


                                                                        </label>
                                                                        <div class="col-md-12">

                                                                            <input type="text"
                                                                                   class="form-control s_website_form_input"
                                                                                   name=" Name" required="1"
                                                                                   placeholder="" id="ob85dx1xebj5"
                                                                                   data-fill-with="name"/>


                                                                        </div>
                                                                    </div>
                                                                    <div class="row s_col_no_resize s_col_no_bgcolor">
                                                                        <label class="col-form-label col-sm-auto s_website_form_label"
                                                                               for="oq4gq1cn0h7g" style="width: 230px;">


                                                                            <span class="s_website_form_label_content">
                                                                                Number
                                                                            </span>


                                                                        </label>
                                                                        <div class="col-md-12">

                                                                            <input type="tel"
                                                                                   class="form-control s_website_form_input"
                                                                                   name=" Number" data-fill-with="phone"
                                                                                   id="oq4gq1cn0h7g"/>


                                                                        </div>
                                                                    </div>
                                                                </div>
                                                                <div class="s_website_form_field mb-3 col-12 s_website_form_model_required"
                                                                     data-type="email" data-name="Field"
                                                                     data-visibility-condition=""
                                                                     data-visibility-between=""
                                                                     style="&#10;    display: flex;&#10;    width: auto;&#10;">
                                                                    <div class="row s_col_no_resize s_col_no_bgcolor">
                                                                        <label class="col-form-label col-sm-auto s_website_form_label"
                                                                               for="o8eml7dzrcqt" style="width: 230px;">


                                                                            <span class="s_website_form_label_content">
                                                                                Email
                                                                            </span>


                                                                        </label>
                                                                        <div class="col-md-12">

                                                                            <input type="email"
                                                                                   class="form-control s_website_form_input"
                                                                                   name="email_from" required="true"
                                                                                   data-fill-with="email"
                                                                                   id="o8eml7dzrcqt"/>


                                                                        </div>
                                                                    </div>
                                                                    <div class="row s_col_no_resize s_col_no_bgcolor">
                                                                        <label class="col-form-label col-sm-auto s_website_form_label"
                                                                               for="odn2l6vo06yd" style="width: 230px;">


                                                                            <span class="s_website_form_label_content">
                                                                                Programme
                                                                            </span>


                                                                        </label>
                                                                        <div class="col-md-12">

                                                                            <input type="text"
                                                                                   class="form-control s_website_form_input"
                                                                                   name="Programme"
                                                                                   data-fill-with="commercial_company_name"
                                                                                   id="odn2l6vo06yd"/>


                                                                        </div>
                                                                    </div>
                                                                </div>
                                                                <div data-visibility-condition=""
                                                                     data-visibility-between=""
                                                                     style="&#10;    display: flex;&#10;"
                                                                     data-type="char" data-name="Field"
                                                                     class="s_website_form_field mb-3 col-12 s_website_form_model_required">
                                                                    <div class="row s_col_no_resize s_col_no_bgcolor">
                                                                        <label class="col-form-label col-sm-auto s_website_form_label"
                                                                               for="owm26x1b4c1" style="width: 230px;">


                                                                            <span class="s_website_form_label_content">
                                                                                Year
                                                                            </span>


                                                                        </label>
                                                                        <div class="col-md-12">

                                                                            <input type="text"
                                                                                   class="form-control s_website_form_input"
                                                                                   name="subject" required="true"
                                                                                   value="" placeholder=""
                                                                                   id="owm26x1b4c1"
                                                                                   data-fill-with="undefined"/>


                                                                        </div>
                                                                    </div>
                                                                    <div class="row s_col_no_resize s_col_no_bgcolor">
                                                                        <label class="col-form-label col-sm-auto s_website_form_label"
                                                                               for="oo21cvep8p2i" style="width: 230px;">


                                                                            <span class="s_website_form_label_content">
                                                                                Current Employment
                                                                            </span>


                                                                        </label>
                                                                        <div class="col-md-12">

                                                                            <input type="text"
                                                                                   class="form-control s_website_form_input"
                                                                                   name="Current Employment" value=""
                                                                                   placeholder="" id="oo21cvep8p2i"
                                                                                   data-fill-with="undefined"/>


                                                                        </div>
                                                                    </div>
                                                                </div>
                                                                <div data-visibility-condition=""
                                                                     data-visibility-between=""
                                                                     class="s_website_form_field mb-3 col-12 s_website_form_custom col-lg-11"
                                                                     data-type="char" data-name="Field">

                                                                    <label class="s_website_form_label"
                                                                           style="width: 230px;padding-left: 15px;"
                                                                           for="owjc0qa78yk">


                                                                        <span class="s_website_form_label_content">
                                                                            Subject
                                                                        </span>


                                                                    </label>

                                                                    <input type="text"
                                                                           class="form-control s_website_form_input"
                                                                           name="Subject" value="" placeholder=""
                                                                           id="owjc0qa78yk" data-fill-with="undefined"
                                                                           style="margin-left: 15px;"/>


                                                                </div>
                                                                <div data-visibility-condition=""
                                                                     data-visibility-between="" data-type="text"
                                                                     data-name="Field"
                                                                     class="s_website_form_field mb-3 col-12 s_website_form_custom s_website_form_required col-lg-11">

                                                                    <label class="s_website_form_label"
                                                                           style="width: 230px;padding-left: 15px;"
                                                                           for="oft7w9uju6df">


                                                                        <span class="s_website_form_label_content">
                                                                            Message
                                                                        </span>


                                                                    </label>

                                                                    <textarea class="form-control s_website_form_input"
                                                                              name="Message" required="1" placeholder=""
                                                                              id="oft7w9uju6df" rows="3"
                                                                              style="&#10;    margin-left: 15px;&#10;"/>


                                                                </div>


                                                                <div class="s_website_form_field mb-3 col-12    s_website_form_dnone"
                                                                     data-name="Field">
                                                                    <div class="row s_col_no_resize s_col_no_bgcolor">
                                                                        <label class="col-form-label col-sm-auto s_website_form_label "
                                                                               style="width: 200px">


                                                                            <span class="s_website_form_label_content"/>


                                                                        </label>
                                                                        <div class="col-sm">

                                                                            <input type="hidden"
                                                                                   class="form-control s_website_form_input"
                                                                                   name="email_to"
                                                                                   value="info@meskc.ac.in"/>


                                                                            <div class="s_website_form_field_description small form-text text-muted">
                                                                                Separate email addresses with a comma.
                                                                            </div>

                                                                        </div>
                                                                    </div>
                                                                </div>

                                                                <div class="mb-0 py-2 col-6 s_website_form_submit text-end s_website_form_no_submit_label"
                                                                     data-name="Submit Button">
                                                                    <div class="s_website_form_label"
                                                                         style="width: 230px;"/>
                                                                    <a href="#" role="button"
                                                                       class="s_website_form_send btn btn-custom text-o-color-3 btn-lg"
                                                                       data-bs-original-title="" title=""
                                                                       style="border-width: 0px; border-style: solid; background-color: rgb(90, 4, 103);">
                                                                        <strong>
                                                                            <span style="font-size: 12px;">
                                                                                ​​​​​​​Subm​​​​it
                                                                            </span>
                                                                        </strong>
                                                                    </a>
                                                                    <span id="s_website_form_result"/>
                                                                </div>
                                                            </div>
                                                        </form>
                                                    </div>
                                                </section>

                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </section>
                        </div>
                    </t>
                </t>
            </field>
        </record>
        <record id="alumni_registration_page" model="website.page">
            <field name="url">/alumni_registration</field>
            <field name="is_published">True</field>
            <field name="view_id" ref="alumni_registration"/>
            <field name="track">True</field>
        </record>
    </data>
</odoo>