<odoo>
    <data>
        <record id="gallery" model="ir.ui.view">
            <field name="name">gallery</field>
            <field name="type">qweb</field>
            <field name="key">mes_college.gallery</field>
            <field name="arch" type="xml">
                <t name="Clubs" t-name="mes_college.gallery_menu">
                    <t t-call="website.layout">
                        <div id="wrap" class="oe_structure">
                            <section class="page o_colored_level" style="padding: 0px; background-image: none;">
