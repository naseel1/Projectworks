<odoo>
    <data>
        <record id="distribution_seats" model="ir.ui.view">
            <field name="name">distribution_seats</field>
            <field name="type">qweb</field>
            <field name="key">mes_college.distribution_seats</field>
            <field name="arch" type="xml">
                <t name="english" t-name="mes_college.distribution_seats_menu">
                    <t t-call="website.layout">
                        <div id="wrap" class="oe_structure">
                            <section class="bg_color">
                                <div class="container" id="container_width">
                                    <div class=" pt32 pb32 col-md-8">
                                        <div>
                                            <h2 class="main-head" style="text-align:left">Distribution of seats</h2>
                                            <div class="content_text" style="text-align:justify;padding: 0;">

                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </section>
                        </div>
                    </t>
                </t>
            </field>
        </record>
        <record id="distribution_seats_page" model="website.page">
            <field name="url">/distribution_seats</field>
            <field name="is_published">True</field>
            <field name="view_id" ref="distribution_seats"/>
            <field name="track">True</field>
        </record>
    </data>
</odoo>