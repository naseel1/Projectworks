<odoo>
    <data>
        <record id="academic_calendar" model="ir.ui.view">
            <field name="name">academic_calendar</field>
            <field name="type">qweb</field>
            <field name="key">mes_college.academic_calendar</field>
            <field name="arch" type="xml">
                <t name="english" t-name="mes_college.academic_calendar_menu">
                    <t t-call="website.layout">
                        <div id="wrap" class="oe_structure">
                            <section class="bg_color">
                                <div class="container" id="container_width">
                                    <div class="row">
                                        <div class=" pt32 pb32 col-md-9">
                                            <div>
                                                <h2 class="main-head" style="text-align:left; padding-top:20px;">
                                                    Academic Calendar
                                                </h2>
                                                <div class="row pt32">
                                                    <section class="academic_calendar">
                                                        <a href="mes_college/static/img/Calender Diary 2022-23.pdf"
                                                           class="readmore"
                                                           id="naac_link">2022-2023
                                                            <span class="fa fa-long-arrow-right text-o-color-3"
                                                                  data-bs-original-title=""
                                                                  title="" aria-describedby="tooltip544395"></span>
                                                        </a>
                                                        <a href="mes_college/static/img/Calendar and Handbook 2021-2022 MESKC.pdf"
                                                           class="readmore" id="naac_link">2021-2022
                                                            <span class="fa fa-long-arrow-right text-o-color-3"
                                                                  data-bs-original-title=""
                                                                  title="" aria-describedby="tooltip544395"></span>
                                                        </a>
                                                        <a href="mes_college/static/img/MES-KC-CalendarBook-2021.pdf"
                                                           class="readmore" id="naac_link">2020-2021
                                                            Cycle
                                                            <span class="fa fa-long-arrow-right text-o-color-3"
                                                                  data-bs-original-title=""
                                                                  title="" aria-describedby="tooltip544395"></span>
                                                        </a>
                                                        <a href="mes_college/static/img/Academic-Calender-converted-2019-20.pdf"
                                                           class="readmore" id="naac_link">2019-2020
                                                            <span class="fa fa-long-arrow-right text-o-color-3"
                                                                  data-bs-original-title=""
                                                                  title="" aria-describedby="tooltip544395"></span>
                                                        </a>
                                                        <a href="mes_college/static/img/Academic-Calender.pdf"
                                                           class="readmore" id="naac_link">Calendar
                                                            <span class="fa fa-long-arrow-right text-o-color-3"
                                                                  data-bs-original-title=""
                                                                  title="" aria-describedby="tooltip544395"></span>
                                                        </a>
                                                    </section>

                                                </div>

                                            </div>

                                        </div>
                                        <div class="col-md-3">
                                            <section class="naac_link">
                                                <a href="/programmes" class="readmore" id="naac_link">Programmes
                                                    <span class="fa fa-long-arrow-right text-o-color-3"
                                                          data-bs-original-title=""
                                                          title="" aria-describedby="tooltip544395"></span>
                                                </a>
                                                <a href="evaluation_policy" class="readmore" id="naac_link">Evaluation
                                                    Policy
                                                    <span class="fa fa-long-arrow-right text-o-color-3"
                                                          data-bs-original-title=""
                                                          title="" aria-describedby="tooltip544395"></span>
                                                </a>
                                                <a href="/academic_monitoring_cell" class="readmore" id="naac_link">
                                                    Academic Monitoring Cell
                                                    <span class="fa fa-long-arrow-right text-o-color-3"
                                                          data-bs-original-title=""
                                                          title="" aria-describedby="tooltip544395"></span>
                                                </a>
                                            </section>

                                        </div>

                                    </div>

                                </div>
                            </section>
                        </div>
                    </t>
                </t>
            </field>
        </record>
        <record id="academic_calendar_page" model="website.page">
            <field name="url">/academic_calendar</field>
            <field name="is_published">True</field>
            <field name="view_id" ref="academic_calendar"/>
            <field name="track">True</field>
        </record>
    </data>
</odoo>