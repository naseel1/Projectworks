            <field name="type">qweb</field>
            <field name="key">emea_college_theme.library_members</field>
            <field name="arch" type="xml">
                <t name="job-1" t-name="emea_college_theme.library_members_menu">
                    <t t-call="website.layout">
                        <div class="bg_color_club" data-original-title="" title="" aria-describedby="tooltip229435">
                            <div class="clubs">LIBRARY MEMBERS</div>
                        </div>
                        <div style="display: flex; justify-content: center;">
                            <section class="former_card">
                                <img src="/emea_college_theme/static/img/alumni4.jpg" alt="Person" class="card-image"
