                                <div class="card-content">
                                    <div class="name">Name Of Professor</div>
                                    <div class="designation">Designation Of Professor
                                        <br/>
                                    </div>
                                </div>
                            </section>
                            <section class="former_card">
                                <img src="/emea_college_theme/static/img/alumni4.jpg" alt="Person" class="card-image"
                                     loading="lazy"/>
                                <div class="card-content">
                                    <div class="name">Name Of Professor</div>
                                    <div class="designation">Designation Of Professor
                                        <br/>
                                    </div>
                                </div>
                            </section>
                            <section class="former_card">
                                <img src="/emea_college_theme/static/img/alumni4.jpg" alt="Person" class="card-image"
                                     loading="lazy"/>
                                <div class="card-content">
                                    <div class="name">Name Of Professor</div>
                                    <div class="designation">Designation Of Professor
                                        <br/>
                                    </div>
                                </div>
                            </section>

                        </div>
                    </t>
                </t>
            </field>
        </record>
        <record id="library_members_page" model="website.page">
            <field name="url">/library</field>
            <field name="is_published">True</field>
            <field name="view_id" ref="library_members"/>
            <field name="track">True</field>
        </record>
    </data>
</odoo>