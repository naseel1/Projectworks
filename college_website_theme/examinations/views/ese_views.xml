<odoo>
    <record id="view_ese_form" model="ir.ui.view">
        <field name="name">ese.form</field>
        <field name="model">end.semester</field>
        <field name="arch" type="xml">
            <form string="End semester Examination">
                <sheet>
                    <group>
                        <field name="title"/>
                        <field name="pdf_file" filename="file_name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="view_ese_tree" model="ir.ui.view">
        <field name="name">ese.tree</field>
        <field name="model">end.semester</field>
        <field name="arch" type="xml">
            <tree string="End Semester Examination">
                <field name="title"/>
                <field name="file_name"/>
            </tree>
        </field>
    </record>

    <record id="action_ese" model="ir.actions.act_window">
        <field name="name">End Semester Examination</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">end.semester</field>
        <field name="view_mode">tree,form</field>
        <field name="view_id" ref="view_ese_tree"/>
    </record>
    <menuitem id="menu_ese"
              name="End Semester Examination"
              parent="menu_examinations_root"
              action="action_ese"
              sequence="12"
              groups="examinations.group_examinations_manage_admin"/>

</odoo>